{
    "cli": {
        "version": ">= 16.20.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "autoIncrement": true,
            "android": {
                "buildType": "apk",
                "env": {
                    "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON"
                }
            }
        },
        "production-store": {
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            }
        }
    },
    "submit": {
        "production": {}
    }
}